    "children": [
      {
        "name": "http_response - Failed HTTP 5XX Code",
        "description": "This alert is fired if an HTTP synthetic check reports an HTTP 5XX code.",
        "type": "MonitorsLibraryMonitorExport",
        "monitorType": "Metrics",
        "evaluationDelay": "0m",
        "notificationGroupFields": [],
        "queries": [
          {
            "rowId": "A",
            "query": "     \n    component=http_response metric=http_response_result_code status_code=4*\n    | count by server, service, environment, location\n"
          }
        ],
        "triggers": [
          {
            "detectionMethod": "StaticCondition",
            "triggerType": "Critical",
            "timeRange": "-5m",
            "threshold": 0,
            "thresholdType": "GreaterThan",
            "field": null,
            "occurrenceType": "Always",
            "triggerSource": "AnyTimeSeries"
          },
